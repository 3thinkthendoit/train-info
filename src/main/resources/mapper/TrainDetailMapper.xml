<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.think.infrastructure.mybits.mapper.ITrainDetailMapper">

    <delete id="deleteTrainDetailByTrainCode" parameterType="map">
        delete from train_detail
        where train_code = #{trainCode}
    </delete>

</mapper>
